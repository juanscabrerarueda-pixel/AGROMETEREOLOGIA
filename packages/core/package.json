{
  "name": "@pkg/core",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts"
    },
    "./schemas.js": {
      "import": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "schemas": ["dist/schemas.d.ts"],
      "schemas.js": ["dist/schemas.d.ts"]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
